{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "diffEditor.experimental.showMoves": true,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "FiraCode Nerd Font Mono",
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.guides.indentation": false,
    "editor.lineHeight": 1.15,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "trailing",
    "editor.rulers": [
        79
    ],
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wordWrap": "on",
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "onFocusChange",
    "files.enableTrash": false,
    "files.trimTrailingWhitespace": true,
    "git.openRepositoryInParentFolders": "never",
    "interactiveWindow.executeWithShiftEnter": true,
    "jupyter.allowUnauthorizedRemoteConnection": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.askForLargeDataFrames": false,
    "jupyter.debugJustMyCode": false,
    "jupyter.enableExtendedPythonKernelCompletions": true,
    "makefile.configureOnOpen": false,
    "mypy-type-checker.reportingScope": "workspace",
    "mypy-type-checker.preferDaemon": true,
    "mypy-type-checker.importStrategy": "fromEnvironment",
    "notebook.lineNumbers": "on",
    "notebook.markup.fontSize": 13,
    "notebook.output.fontSize": 11,
    "notebook.output.scrolling": true,
    "notebook.output.textLineLimit": 30,
    "notebook.output.wordWrap": true,
    "python.terminal.activateEnvironment": false,
    "redhat.telemetry.enabled": true,
    "telemetry.telemetryLevel": "error",
    "terminal.integrated.altClickMovesCursor": false,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.terminal.toggleTerminal",
        "workbench.action.toggleMaximizedPanel"
    ],
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "update.mode": "manual",
    "window.autoDetectColorScheme": true,
    "window.customMenuBarAltFocus": false,
    "window.density.editorTabHeight": "compact",
    "window.enableMenuBarMnemonics": false,
    "workbench.activityBar.location": "top",
    "workbench.editor.enablePreview": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.preventPinnedEditorClose": "never",
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.editor.tabActionUnpinVisibility": false,
    "workbench.editor.tabSizing": "fit",
    "workbench.editor.tabSizingFixedMaxWidth": 250,
    "workbench.editor.tabSizingFixedMinWidth": 200,
    "workbench.editor.wrapTabs": true,
    "workbench.list.openMode": "doubleClick",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#a0a0f820"
    },
}